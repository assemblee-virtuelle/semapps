{
  "name": "semapps-frontend",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "nx run-many --target=build --all && yarn yalc:publish",
    "version": "lerna version --force-publish='*' --exact --tag-version-prefix='frontend-v'",
    "yalc:publish": "lerna exec --parallel --scope @semapps/** -- yalc publish --push --changed",
    "link-shape-packages": "lerna exec --parallel -- yalc link @activitypods/ldo-shapes @activitypods/shape-definitions",
    "unlink-shape-packages": "yalc remove --all && rm -rf node_modules/@activitypods/ldo* node_modules/@activitypods/shape*  && yarn install --force",
    "publish": "lerna publish from-package --dist-tag latest",
    "prettier": "prettier --write '**/*.{js,ts,json,tsx,jsx}'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint-files": "eslint --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "preinstall": "git config core.hooksPath .git-hooks",
    "test-watch": "nx watch --all -- nx run $NX_PROJECT_NAME:build"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@parcel/packager-ts": "^2.13.3",
    "@parcel/transformer-typescript-types": "^2.13.3",
    "@types/react": "^18.2.14",
    "@types/shexj": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-typescript": "^0.0.4",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "lerna": "^8.0.1",
    "nodemon": "^3.0.1",
    "nx": "^17.2.5",
    "parcel": "^2.13.3",
    "prettier": "^3.1.1",
    "ts-migrate": "^0.1.35",
    "typescript": "^5.1.6",
    "yalc": "1.0.0-pre.53"
  },
  "workspaces": [
    "packages/*",
    "templates/**"
  ],
  "version": "0.4.0-alpha.37",
  "dependencies": {}
}
